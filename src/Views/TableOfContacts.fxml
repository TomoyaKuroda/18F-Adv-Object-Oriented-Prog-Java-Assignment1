                  <TableColumn fx:id="lastNameColumn" prefWidth="96.79998779296875" text="Last Name" />
                  <TableColumn fx:id="addressColumn" prefWidth="172.79998779296875" text="Address" />
                  <TableColumn fx:id="phoneColumn" prefWidth="127.20001220703125" text="Phone" />
              </columns>
            </TableView>
            <Button mnemonicParsing="false" onAction="#changeToRegisterButtonPushed" text="create new contact" />
            <Label fx:id="messageLabel" textFill="RED">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
